<script language="JavaScript">
function showLanguageSwitchSeparator(id) {
document
.getElementById("btn_separator")
.classList.remove("lang-selector-separator-inactive");
document
.getElementById("button_" + id)
.classList.remove("lang-selector-hide");
}
function hideAllContainersForAllLanguages() {
var allLanguageBoxContainers = document.getElementsByClassName(
"container-body-wrapper"
);
Array.from(allLanguageBoxContainers).forEach(function (o) {
o.classList.add("container-body-wrapper-hide");
});
}
function deactiveAllSwitchesForAllOtherLanguages() {
var allLanguageSelectorButtons = document.getElementsByClassName(
"btn-language-selector-wrapper"
);
Array.from(allLanguageSelectorButtons).forEach(function (o) {
o.classList.add("lang-selector-inactive");
});
Array.from(allLanguageSelectorButtons).forEach(function (o) {
o.classList.remove("lang-selector-active");
});
}
function showLanguageContainer(id) {
document
.getElementById(id)
.classList.remove("container-body-wrapper-hide");
}
function activateLanguageSwitch(id) {
var button = document.getElementById("button_" + id);
button.classList.remove("lang-selector-inactive");
button.classList.add("lang-selector-active");
}
function changeLanguage(id) {
hideAllContainersForAllLanguages();
deactiveAllSwitchesForAllOtherLanguages();
showLanguageContainer(id);
activateLanguageSwitch(id);
}
function initForNewScreens(id) {
showLanguageSwitchSeparator(id);
changeLanguage(id);
}
// init method is in STYLE
</script>
